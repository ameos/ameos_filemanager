{
    "name": "ameos/ameos_filemanager",
    "description": "This will allow you to fully handle file upload in FE context.",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/ameos/ameos_filemanager",
    "require": {
        "typo3/cms-core": "^13",
        "typo3/cms-fluid": "^13",
        "typo3/cms-extbase": "^13",
        "php": ">=8.0.0"
    },
    "keywords": [
        "TYPO3",
        "CMS",
        "EDM",
        "File Manager"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ameos Team",
            "email": "typo3dev@ameos.com",
            "homepage": "http://www.ameos.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ameos\\AmeosFilemanager\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ameos_filemanager"
        }
    }
}
