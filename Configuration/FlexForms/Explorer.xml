                        <TCEforms>
                          <exclude>1</exclude>
                          <label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xlf:flexform.authorizedCategories</label>
                          <config>
                            <type>select</type>
                            <foreign_table>sys_category</foreign_table>
                            <foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.title ASC</foreign_table_where>
