<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sheetStd>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.generalSheet</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.stockageGroupPid>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.stockageGroupPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.stockageGroupPid>
					<settings.storage>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.storage</label>
							<config>
								<type>select</type>
								<internal_type>db</internal_type>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<foreign_table>sys_file_storage</foreign_table>
								<foreign_table_where>ORDER BY sys_file_storage.name</foreign_table_where>
							</config>
						</TCEforms>
					</settings.storage>
					<settings.startFolder>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.startFolder</label>
							<config>
								<type>select</type>
								<foreign_table>tx_ameosfilemanager_domain_model_folder</foreign_table>
								<size>30</size>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<renderMode>tree</renderMode>
								<treeConfig>
									<expandAll>true</expandAll>
									<parentField>uid_parent</parentField>
									<appearance>
										<showHeader>true</showHeader>
									</appearance>
								</treeConfig>
							</config>
						</TCEforms>
					</settings.startFolder>
					<settings.allowedFileExtension>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.allowedFileExtension</label>
							<config>
								<type>input</type>
								<default>jpg,jpeg,tif,png,gif,pdf,docx,doc,xls,csv,odt,ppt</default>
							</config>
						</TCEforms>
					</settings.allowedFileExtension>
				</el>
			</ROOT>
		</sheetStd>
		<sheetAppearance>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.sheetAppearance</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.columnsTable>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.columnsTable</label>
							<config>
								<type>select</type>
								<multiple>false</multiple>
								<size>12</size>
								<minitems>1</minitems>
								<maxitems>11</maxitems>
								<items type="array">
							        <numIndex index="1" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.icon</numIndex>
							            <numIndex index="1">icon</numIndex>
							        </numIndex>
							        <numIndex index="2" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.title</numIndex>
							            <numIndex index="1">title</numIndex>
							        </numIndex>
							        <numIndex index="3" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.createdAt</numIndex>
							            <numIndex index="1">createdAt</numIndex>
							        </numIndex>
							        <numIndex index="4" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.updatedAt</numIndex>
							            <numIndex index="1">updatedAt</numIndex>
							        </numIndex>
							        <numIndex index="5" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.description</numIndex>
							            <numIndex index="1">description</numIndex>
							        </numIndex>
							        <numIndex index="6" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.owner</numIndex>
							            <numIndex index="1">owner</numIndex>
							        </numIndex>
							        <numIndex index="7" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.size</numIndex>
							            <numIndex index="1">size</numIndex>
							        </numIndex>
							        <numIndex index="8" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.keywords</numIndex>
							            <numIndex index="1">keywords</numIndex>
							        </numIndex>
							        <numIndex index="9" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.actions</numIndex>
							            <numIndex index="1">actions</numIndex>
							        </numIndex>
							        <numIndex index="10" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.categories</numIndex>
							            <numIndex index="1">categories</numIndex>
							        </numIndex>
					            </items>
								<default>icon,title,updatedAt,size,actions</default>
							</config>
						</TCEforms>
					</settings.columnsTable>
					<settings.actionDetail>
							<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.actionDetail</label>
							<config>
								<type>select</type>
								<multiple>false</multiple>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>4</maxitems>
								<items type="array">
							        <numIndex index="1" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.download</numIndex>
							            <numIndex index="1">download</numIndex>
							        </numIndex>
							        <numIndex index="2" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.detail</numIndex>
							            <numIndex index="1">detail</numIndex>
							        </numIndex>
							        <numIndex index="3" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.edit</numIndex>
							            <numIndex index="1">edit</numIndex>
							        </numIndex>
							        <numIndex index="4" type="array">
							            <numIndex index="0">LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.delete</numIndex>
							            <numIndex index="1">delete</numIndex>
							        </numIndex>
					            </items>
								<default>download,detail,edit</default>
							</config>
						</TCEforms>
					</settings.actionDetail>
					<settings.iconFolder>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.iconFolder</label>
							<config>
								<type>input</type>
								<default>typo3conf/ext/ameos_filemanager/Resources/Public/Icons/</default>
							</config>
						</TCEforms>
					</settings.iconFolder>
				</el>
			</ROOT>
		</sheetAppearance>
		<sheetEdition>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.sheetEdition</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.authorizedGroups>
						<TCEforms>
							<label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.authorizedGroup</label>
							<config>
								<type>select</type>
								<foreign_table>fe_groups</foreign_table>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>999</maxitems>
								<items>
									<numIndex index="1" type="array">
							            <numIndex index="0">LLL:EXT:lang/locallang_general.xlf:LGL.any_login</numIndex>
							            <numIndex index="1">-2</numIndex>
						            </numIndex>
						            <numIndex index="2" type="array">
							            <numIndex index="0">LLL:EXT:lang/locallang_general.xlf:LGL.usergroups</numIndex>
							            <numIndex index="1">--div--</numIndex>
						            </numIndex>
						        </items>
							</config>
						</TCEforms>
					</settings.authorizedGroups>
					<settings.authorizedCategories>
						<TCEforms>
						  <exclude>1</exclude>
						  <label>LLL:EXT:ameos_filemanager/Resources/Private/Language/locallang_be.xml:flexform.authorizedCategories</label>
						  <config>
							<type>select</type>
							<autoSizeMax>50</autoSizeMax>
							<foreign_table>sys_category</foreign_table>
							<foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.title ASC</foreign_table_where>
							<maxitems>9999</maxitems>
							<renderMode>tree</renderMode>
							<size>10</size>
							<treeConfig>
								<appearance>
									<expandAll>1</expandAll>
									<showHeader>1</showHeader>
								</appearance>
								<parentField>parent</parentField>
							</treeConfig>
						  </config>
						</TCEforms>
					</settings.authorizedCategories>
				</el>
			</ROOT>
		</sheetEdition>
	</sheets>
</T3DataStructure>